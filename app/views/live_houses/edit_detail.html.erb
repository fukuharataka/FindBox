<div class="full_screen_container b">
  <%= render 'layouts/header' %>
  <div class="edit_container b">
    <%= form_for(@live_house, url: update_detail_path(@live_house)) do |f| %>
    <%= f.hidden_field :detail, id: :live_house_detail %>
